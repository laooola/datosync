{
  "name": "datosync",
  "version": "1.0.0",
  "description": "a simple tool to pull, diff and synchronize dato cms models and fields",
  "keywords": [
    "diff",
    "pull",
    "sync",
    "dato cms",
    "schema",
    "models",
    "fields"
  ],
  "license": "ISC",
  "author": "Patrick Schedler",
  "repository": {
    "type": "git",
    "url": "https://github.com/laooola/datosync.git"
  },
  "bugs": "https://github.com/laooola/datosync/issues",
  "bin": "./bin/datosync",
  "engines": {
    "node": ">=7.6.0"
  },
  "scripts": {
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "commander": "2.20.0",
    "datocms-client": "1.0.3",
    "diff": "4.0.1",
    "kleur": "3.0.3",
    "lodash.isequal": "4.5.0",
    "lodash.omit": "4.5.0"
  },
  "devDependencies": {
    "husky": "2.2.0",
    "jest": "24.8.0",
    "lint-staged": "8.1.6",
    "nock": "10.0.6",
    "prettier": "1.17.0"
  }
}
